<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="formacionView" spacing="5.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="formacionTableView" editable="true" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="desdeColumn" prefWidth="75.0" text="Desde" />
          <TableColumn fx:id="hastaColumn" prefWidth="75.0" text="Hasta" />
            <TableColumn fx:id="denominacionColumn" prefWidth="100.0" text="Denominación" />
            <TableColumn fx:id="organizadorColumn" prefWidth="75.0" text="Organizador" />
        </columns>
      </TableView>
      <VBox spacing="5.0">
         <children>
            <Button fx:id="addFormacion" mnemonicParsing="false" onAction="#onAddFormacionAction" prefWidth="58.4" text="Añadir" />
            <Button fx:id="deleteFormacion" mnemonicParsing="false" onAction="#onDeleteFormacionAction" text="Eliminar" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>